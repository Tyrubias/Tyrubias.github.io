{
    "name": "victor-personal-website",
    "version": "1.0.0",
    "description": "Victor Song's personal website",
    "repository": {
        "type": "git",
        "url": "https://github.com/Tyrubias/Tyrubias.github.io/"
    },
    "license": "MIT",
    "scripts": {
        "build": "parcel build src/index.html --no-source-maps --experimental-scope-hoisting --public-url ./build -d ./build && ln -sv build/index.html index.html",
        "start": "parcel -d ./build src/index.html",
        "format": "npx prettier --write \"src/**/*.{js,svelte,html,css,scss,json}\"",
        "clean": "rm -f *.js *.css *.html *.webp *.jpg && rm -rf .cache/ && rm -rf build/"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-svelte": "^4.0.6",
        "prettier": "^2.0.5",
        "prettier-plugin-svelte": "^1.1.0",
        "svelte": "^3.23.2"
    },
    "dependencies": {
        "svelte-spa-router": "^2.2.0"
    }
}
